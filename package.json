{
  "name": "astro-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "npm run build:studio && npm run copy:studio && astro build",
    "build:studio": "cd studio && npm run build -- --basePath /studio",
    "copy:studio": "node -e \"const f=require('fs');f.rmSync('public/studio',{recursive:true,force:true});f.cpSync('studio/dist','public/studio',{recursive:true});\"",
    "postinstall": "cd studio && npm install",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@portabletext/react": "^6.0.2",
    "@sanity/client": "^7.15.0",
    "@sanity/image-url": "^2.0.3",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.17.1",
    "framer-motion": "^12.34.0",
    "lucide-react": "^0.569.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18"
  },
  "overrides": {
    "glob": "^11.0.1",
    "whatwg-encoding": "npm:@exodus/bytes@^1.0.1"
  }
}
